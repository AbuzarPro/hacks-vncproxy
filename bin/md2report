# begin-cgir titlepage.tex cat
\begin{titlepage}
  \makeatletter
  \null\vfil
  \vskip 60\p@
  \begin{center}%
    {\larger[4]\bfseries \@title \par}%
    \vskip 5em%
    {\larger[3]%
      \begin{tabular}[t]{c}%
        \@author
      \end{tabular}%
      \par%
    }%    
    \vskip 5em%
    {\larger[2] \@date \par}%
    \vfill
    {\ifx\titleimg\undefined\else
     \includegraphics[width=.75\textwidth]{\titleimg}\fi}
  \end{center}\par
  \vfill
  \null\hspace{2em}%
  \includegraphics[width=3cm]{logo-univ-lille1}%
  \hfill%
  \includegraphics[width=5cm]{logo-cgir}%
  \hspace{2em}\null
  \vskip 60\p@
  \makeatother
\end{titlepage}
