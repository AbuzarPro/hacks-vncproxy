# begin-lille1 titlepage.tex cat
\begin{titlepage}
  \makeatletter
  \null\vfil
  \vskip 60\p@
  \begin{center}%
    {\larger[4]\bfseries \@title \par}%
    \vskip 5em%
    {\larger[3]%
      \begin{tabular}[t]{c}%
        \@author
      \end{tabular}%
      \par%
    }%    
    \vskip 5em%
    {\larger[2] \@date \par}%
    \vfill
    {\ifx\titleimg\undefined\else
     \includegraphics[width=.75\textwidth]{\titleimg}\fi}
  \end{center}\par
  \vfill
  \centerline{\includegraphics[width=3cm]{logo-univ-lille1}}
  \vskip 60\p@
  \makeatother
\end{titlepage}
# end titlepage.tex

